<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="register">
    <select id="registCheck" parameterType="String" resultType="java.util.String">
        SELECT 	EMAIL
        FROM 	USER
        WHERE	EMAIL = #{email}
    </select>
    
    <select id="regist" parameterType="com.hybrid.gungduk.dto" resultType="int">
        INSERT 
        	INTO USER(email, pw, phoneNum)
        VALUES(#{email}, #{pw}, #{phoneNum})
    </select>
</mapper>